   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            category="gwt.junit"
            class="com.google.gwt.eclipse.core.launch.ui.GWTJUnitLaunchShortcut"
            description="Run a GWT JUnit Test"
            icon="icons/gwt-junit_small.gif"
            id="com.google.gwt.eclipse.core.launch.gwtJUnitShortcut"
            label="GWT JUnit Test"
            modes="run, debug">
            
			<contextualLaunch>
            	<enablement>
	               	<with variable="selection">
	                	<count value= "1"/>
	                    <iterate>
	                    	<test property="com.google.gwt.launching.isGWTTest"/>
	                  	</iterate>
	              	</with>
            	</enablement>
         	</contextualLaunch>
         <description
               description="Runs a GWT JUnit Test"
               mode="run">
         </description>
         <description
               description="Debugs a GWT JUnit Test"
               mode="debug">
         </description>
         <configurationType
               id="com.google.gwt.eclipse.core.launch.gwtJUnit">
         </configurationType>
      </shortcut>
      <shortcut
            category="gwt.junit"
            class="com.google.gwt.eclipse.core.launch.ui.GWTJUnitWebLaunchShortcut"
            description="Run a GWT JUnit Test"
            icon="icons/gwt-junit_small.gif"
            id="com.google.gwt.eclipse.core.launch.gwtJUnitWebShortcut"
            label="GWT JUnit Test (production mode)"
            modes="run, debug">

			<contextualLaunch>
            	<enablement>
	               	<with variable="selection">
	                	<count value= "1"/>
	                    <iterate>
	                    	<test property="com.google.gwt.launching.isGWTTest"/>
	                  	</iterate>
	              	</with>
            	</enablement>
         	</contextualLaunch>
         <description
               description="Runs a GWT JUnit Test"
               mode="run">
         </description>
         <description
               description="Debugs a GWT JUnit Test"
               mode="debug">
         </description>
         <configurationType
               id="com.google.gwt.eclipse.core.launch.gwtJUnit">
         </configurationType>
      </shortcut>
   </extension>
   <!--
