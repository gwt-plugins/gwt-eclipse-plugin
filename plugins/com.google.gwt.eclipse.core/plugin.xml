   <extension
         point="org.eclipse.ltk.core.refactoring.renameParticipants">
      <renameParticipant
            class="com.google.gwt.eclipse.core.refactoring.GWTRenameTypeInJsniParticipant"
            id="com.google.gwt.eclipse.core.refactoring.renameTypeInJsniParticipant"
            name="Rename type in JSNI blocks">
         <enablement>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IType">
               </instanceof>
            </with>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="com.google.gwt.eclipse.core.refactoring.GWTRenameMemberInJsniParticipant"
            id="com.google.gwt.eclipse.core.refactoring.renameMemberInJsniParticipant"
            name="Rename member in JSNI blocks">
         <enablement>
            <with
                  variable="element">
               <or>
               <instanceof
                     value="org.eclipse.jdt.core.IMethod">
               </instanceof>
               <instanceof
                     value="org.eclipse.jdt.core.IField">
               </instanceof>
            </or>
            </with>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="com.google.gwt.eclipse.core.refactoring.rpc.PairedInterfaceRenameParticipant"
            id="com.google.gwt.eclipse.core.refactoring.rpc.pairedInterfaceRenameParticipant"
            name="Rename a GWT RPC paired interface">
         <enablement>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IType">
               </instanceof>
            </with>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="com.google.gwt.eclipse.core.refactoring.rpc.PairedMethodRenameParticipant"
            id="com.google.gwt.eclipse.core.refactoring.rpc.pairedMethodRenameParticipant"
            name="Rename a method in a GWT RPC paired interface">
         <enablement>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IMethod">
               </instanceof>
            </with>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
         </enablement>
      </renameParticipant>
   </extension>
   <extension
         point="org.eclipse.ltk.core.refactoring.deleteParticipants">
      <deleteParticipant
            class="com.google.gwt.eclipse.core.refactoring.GWTDeleteCompilationUnitParticipant"
            id="com.google.gwt.eclipse.core.refactoring.deleteCompilationUnitParticipant"
            name="Delete GWT Java reference index entries">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.ICompilationUnit">
               </instanceof>
            </with>
         </enablement>
      </deleteParticipant>
   </extension>
