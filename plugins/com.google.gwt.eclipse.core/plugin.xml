            file-extensions="java"
            id="gwtJavaSource"
            name="GWT Java Source File"
            priority="high">
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            id="com.google.gwt.eclipse.core.uibinder.content.xml"
            name="UiBinder Template"
            priority="high">
         <describer class="com.google.gwt.eclipse.core.uibinder.UiBinderXmlContentDescriber">
         <parameter name="element" value="UiBinder" />
         </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.wst.css.core.csssource"
            id="com.google.gwt.eclipse.core.clientbundle.content.css"
            name="CSS Resource file"
            priority="high">
