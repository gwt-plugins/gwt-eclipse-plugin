    <extension
        point="org.eclipse.debug.ui.launchShortcuts">
        <shortcut
            class="com.google.gwt.eclipse.core.speedtracer.ui.SpeedTracerLaunchShortcut"
            description="Profile Using Speed Tracer"
            icon="icons/speed-tracer_small.png"
            id="com.google.gwt.eclipse.core.speedtracer.speedTracerLaunchShortcut"
            label="Speed Tracer"
            modes="profile">
            <contextualLaunch>
                <enablement>
                    <with
                        variable="selection">
                        <count
                            value="1" />
                        <iterate>
                            <!-- TODO: create a launch tester for ST -->
                            <!--
                                see code in
                                com.google.gdt.eclipse.suite.propertytesters.LaunchTargetTester.java
                            -->
                            <test
                                property="com.google.gdt.eclipse.suite.isLaunchTarget" />
                        </iterate>
                    </with>
                </enablement>
            </contextualLaunch>

            <description
                description="Profile Using Speed Tracer"
                mode="profile" />
            <configurationType
                id="com.google.gwt.eclipse.core.speedtracer.speedTracerLaunch" />
        </shortcut>
    </extension>
