      </editor>
      <editor
            class="com.google.gwt.eclipse.core.uibinder.sse.UiBinderXmlTextEditor"
            id="com.google.gwt.eclipse.core.editors.uiBinderXmlEditor"
            icon="icons/gwt-uibinderxml-file_small.gif"
            name="UiBinder Template Editor">
         <contentTypeBinding
               contentTypeId="com.google.gwt.eclipse.core.uibinder.content.xml">
